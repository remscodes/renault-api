{
  "name": "@remscodes/renault-api",
  "version": "1.2.2",
  "description": "Resources url and models to use the Renault API",
  "license": "MIT",
  "author": {
    "name": "RÃ©my Abitbol",
    "url": "https://github.com/remscodes"
  },
  "scripts": {
    "build": "rollup -c",
    "types": "tsc --noEmit",
    "rm-dist": "node ./scripts/rm-dist.cjs",
    "prebuild": "npm run rm-dist"
  },
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "node": "./dist/cjs/index.cjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.mjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/remscodes/renault-api"
  },
  "engines": {
    "node": ">=18.13.0"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.10.3",
    "esbuild": "^0.19.8",
    "rollup": "^4.6.1",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "renault"
  ],
  "packageManager": "pnpm@8.10.5"
}
